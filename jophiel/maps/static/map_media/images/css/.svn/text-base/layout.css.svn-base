*{margin:0;padding:0;}
html{background: #FFF;}
body { text-align: center; margin:0; padding:0; background: #FFF; color:#000;font-family:Verdana, Lucida, Arial, Helvetica,"宋体",sans-serif;}
form{margin:0;padding:0;}
.clear { clear:both;font-size:1px; height:0; visibility: hidden; }
.c{padding:0;text-align:center;}
img{border:0;}
.hidden {display: none;}
.transparent{background:transparent;}
em{font:13px/100% "宋体",Arial,sans-serif;letter-spacing:1px;}
em.statushalt{color: #c00;font-style: normal;line-height:150%}
em.statusnatural{	color: #060;font-style: normal;line-height:150%}
em.explain{color: #993300;font-style: normal;}
em.error{color:#f00;font-style:normal;padding:0 10px;}
em.star{font-size:15px;color:#930;font-style: normal;padding:0 10px;line-height:70%}


/*navigation start*/
div.navigation {width:100%; margin-top:2px;}
div.navigation div.button{ float:left;}
div.navigation div.page{ float:right;text-align:right;vertical-align: middle;font:12px/120% "";}
div.navigation div.page em{ color:#c00;font-style: normal;}
div.navigation div.page em.statushalt{color: #c00;font-style: normal;}
div.navigation div.page em.statusnatural{	color: #060;font-style: normal;}
div.navigation div.page span{ vertical-align: super !important;vertical-align:baseline;margin:3px !important;margin:0px; overflow: hidden; height: 18px; border:1 solid #000;}
div.navigation div.page span select{ margin:-2px;font-size:12px;height:17px !important; color: #000; border:1px solid #000}

table.classicLook td{font:12px/150% "宋体",Arial,sans-serif;}
table.classicLook td a{font:13px/150% "宋体",Arial,sans-serif;letter-spacing:2px;padding:0 1px;}
