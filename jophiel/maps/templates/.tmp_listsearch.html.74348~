	<script language="JavaScript">
		function getNewSubmitForm(){
			var submitForm = document.createElement("FORM");
			document.body.appendChild(submitForm);
			submitForm.method = "GET";
			return submitForm;
		}
　		function createNewFormElement(inputForm, elementName, elementValue){
			var newElement=document.createElement("\<input name=" +elementName+"\>");
			
			//var newElement= document.createElement("<input name="+elementName+" >");
			inputForm.appendChild(newElement);
			newElement.value = elementValue;
			newElement.type='hidden'
			return newElement;
		}	　	
		function createFormAndSubmit(action,form,pguid){
			var submitForm = getNewSubmitForm();
			createNewFormElement(submitForm, "cityname", form.cityname);
			createNewFormElement(submitForm, "pguid", form.pguid);
			createNewFormElement(submitForm, "enginechoose", form.enginechoose);
			submitForm.action=action ;
			submitForm.submit();
		}
	
		function　showMsg(event,pguid){
	　　　　 form=document.forms["searchform"];
	      	createFormAndSubmit("/detail",form,pguid);
	　　}
		function showM(event){
			alert("hello");
		}	
	</script>
	
