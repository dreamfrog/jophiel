	<table id="body_table" width="100%" border="0" cellpadding="0" cellspacing="0" align="left" class="bi">
		<tr valign="top" alingn="left">
		<td>
		<div height="680">
	  	{% for search in searchresult %}
	  			 <div class="bg_dotted" onmouseover="this.style.backgroundColor='#f4f9ff';" onmouseout="this.style.backgroundColor='#D9E1F7'">
	    			<table>
	    				<tr><td>
	    					<strong class="STYLE2"><img src="/images/sort/{{forloop.counter}}.bmp"> 
	    					<a class="STYLE2" href="/detail?cityname={{cityname}}&pguid={{search.pguid}}" target=_blank > 
	    					<span onmouseover="show_tip('{{search.x}}','{{search.y}}','{{forloop.counter}}')">{{search.name}}
	    					{% if search.ename %}
	    					（{{search.ename}}）
	    					{% endif %}
	    					</span></a></strong>   				
		    			</td></tr>
		  				<tr><td>
		  					<table>	  							  			
			  					<tr class="STYLE9" valign="top">
				  					<td>
										地址：{{ search.address}}
										{% if search.eaddress %}
											（{{search.eaddress}}）
										{% endif %}
									</td>
								</tr>
								<tr class="STYLE9" valign="top">
				  					<td>
										电话：{{ search.tel}}
									</td>
								</tr>
								<tr class="STYLE9" valign="top">
				  					<td>
										类型：{{ search.type}}
									</td>
								</tr>										
							</table>
						</td></tr>		
				</table>
			</div>
		{% endfor %}
		</div>
		
	</td>
	</tr>
	</table>
